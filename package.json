{
  "name": "relatos",
  "version": "0.1.0",
  "description": "Relatos is a relational atlas visualization library developed by Human Histories.",
  "main": "./dist/relatos.umd.js",
  "module": "./dist/relatos.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/relatos.es.js",
      "require": "./dist/relatos.umd.js"
    }
  },
  "files": [
    "dist/relatos.umd.js",
    "dist/relatos.es.js",
    "dist/index.d.ts",
    "LICENSE",
    "NOTICE",
    "README.md"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepack": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": ["relatos", "visualization", "graph", "map", "globe", "atlas", "cesium", "leaflet"],
  "author": "Human Histories",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/humanhistories/relatos.git"
  },
  "bugs": {
    "url": "https://github.com/humanhistories/relatos/issues"
  },
  "homepage": "https://humanhistories.org/en/histoverse/#relatos",
  "dependencies": {
    "cesium": "^1.137.0",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-cesium": "^1.2.23",
    "vite-plugin-dts": "^4.0.0",
    "rimraf": "^5.0.5",
    "@types/leaflet": "^1.9.8"
  }
}
